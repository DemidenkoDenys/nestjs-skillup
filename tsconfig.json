{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": true,
    "esModuleInterop": true
  },
  "exclude": [
    "dist",
    "node_modules",
    "_module_prototype_",
  ]
}
